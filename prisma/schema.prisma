// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Ticket {
  gameId  String  @unique @map("game_id")
  title   String
  code    String
  exposed Boolean @default(false)

  @@map("ticket")
}
